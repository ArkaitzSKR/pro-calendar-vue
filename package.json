{
  "name": "@lbgm/pro-calendar-vue",
  "version": "1.1.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-lib": "npm run build-scss && vite build && vue-tsc --emitDeclarationOnly --allowJs && mv dist/src dist/types",
    "build-scss": "npx tailwindcss -i ./src/assets/index.scss -o ./src/assets/tailwind.scss"
  },
  "engines": {
    "node": ">=14.18.3",
    "npm": ">=6.14.15"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "./dist/pro-calendar-vue.umd.js",
  "module": "./dist/pro-calendar-vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/pro-calendar-vue.es.js",
      "require": "./dist/pro-calendar-vue.umd.js"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "pinia": "^2.0.23",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.18.3",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8"
  },
  "keywords": [
    "vuejs",
    "pro calendar",
    "typescript",
    "vite",
    "lbgm",
    "vue component",
    "calendar",
    "professional",
    "professional calendar"
  ],
  "description": "Professional Calendar for Vue 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbgm/Pro-Calendar-VueJs.git"
  },
  "author": "@lbgm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lbgm/Pro-Calendar-VueJs/issues"
  },
  "homepage": "https://github.com/lbgm/Pro-Calendar-VueJs#readme"
}
